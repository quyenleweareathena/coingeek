<div class="home__section_content news__wrapper">
    <div class="wrapper">
        <div class="week__section">
            <div class="title">
                <h2 class="title__section">Coingeek Week</h2>
            </div>
            <div class="row">
                <div class="latest__new--left col-6">
                    <div class="row">
                        {% for i in 0..1 %}
                            {% include "../components/_news.twig" with {'col': 12} %}
                        {% endfor %}
                    </div>
                </div>
                <div class="latest__new--right col-6">
                    <div class="row">
                        <div class="col-6">
                            <div class="lists__coingeek--week">
                                {% for i in 0..1 %}
                                    {% include "../components/_coingeek-week-item-list.twig" %}
                                {% endfor %}
                            </div>
                        </div>
                        <div class="col-6">

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

