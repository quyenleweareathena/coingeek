<div class="home__section_content news__wrapper">
    <div class="wrapper">
        <div class="week__section">
            <div class="title">
                <h2 class="title__section">Coingeek Week</h2>
            </div>
            <div class="row">
                <div class="latest__new--left col-6">
                    <div class="row">
                        {% for i in 0..1 %}
                            {% include "../components/_news.twig" with {'col': 12} %}
                        {% endfor %}
                    </div>
                </div>
                <div class="latest__new--right col-6">
                    <div class="include-rigister cg__radius">
                        <p class="title"><span>CG</span> week</p>
                        <p class="description">Coingeek Week
                            Conference</p>
                        <p class="date">London - May 2019</p>
                        <div class="button-discover">
                            <a href="#">Discover Coingeek Week</a>
                        </div>
                        <div class="button-register now">
                            <a href="#">Pre-Register now</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

